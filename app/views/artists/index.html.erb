
<table>
  <tbody>
    <tr>
      <td>Artist Name</td>
      <td>No of songs</td>
    </tr>
    <% @artists.each do |artist| %>
      <tr>
        <td> <%= link_to "#{artist.name}", artist_path(artist) %></td>
        <td> <%= artist.songs.count %></td>
      <tr>
    <% end %>
  </tbody>
</table>   


